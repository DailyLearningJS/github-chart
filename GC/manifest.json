{
    "name": "GitHub-Chart",
    "version": "1.0.2",
    "description": "Renders an new face for GitHub contribution graphs.",
    "content_scripts": [{
        "css": ["gc-min.css"],
        "js": ["jquery.js", "gc-min.js"],
        "matches": ["https://github.com/*", "http://localhost:8080/dev/demo.html"]
    }],
    "permissions": [
        "storage"
    ],
    "icons": {
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "manifest_version": 2
}
